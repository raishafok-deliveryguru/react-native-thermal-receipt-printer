{
	"name": "choiceqr-react-native-thermal-printer",
	"version": "2.1.0",
	"description": "Fork of react-native-thermal-receipt-printer which is fork of react-native-printer. A React Native Library to support USB/BLE/Net printer",
	"main": "dist/index.js",
	"source": "dist/index.js",
	"scripts": {
		"build": "rimraf ./dist && tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/karaushu/react-native-thermal-receipt-printer.git"
	},
	"files": [
		"dist",
		"android",
		"ios",
		"src",
		"choiceqr-react-native-thermal-printer.podspec"
	],
	"keywords": [
		"react-native",
		"thermal",
		"printer",
		"bluetooth",
		"net",
		"printer",
		"rn",
		"android"
	],
	"dependencies": {
		"buffer": "^5.7.1",
		"iconv-lite": "^0.6.2",
		"jimp": "^0.22.12",
		"string_decoder": "^1.3.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.10",
		"@types/node": "^14.6.0",
		"@types/react": "~18.0.27",
		"@types/react-native": "~0.70.6",
		"@types/react-test-renderer": "^16.9.3",
		"pod-install": "^0.1.10",
		"react": "18.2.0",
		"react-native": "0.71.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.9.5"
	},
	"author": "karaushu",
	"homepage": "https://github.com/karaushu/react-native-thermal-receipt-printer#readme",
	"license": "ISC",
	"codegenConfig": {
		"name": "RNPrinterSpecs",
		"type": "all",
		"jsSrcsDir": "src",
